{
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-xelatex",
          "-synctex=1",
          "-file-line-error",
          "-interaction=nonstopmode",
          "--output-directory=build",
          "--shell-escape",
          "%DOC%"
        ]
      }
    ],
    "cSpell.words": [
      "parallelized",
      "resampling"
    ]
  }
  