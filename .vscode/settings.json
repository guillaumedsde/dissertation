{
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-pdf",
          "-synctex=1",
          "-interaction=nonstopmode",
          "--output-directory=build",
          "-file-line-error",
          // "--shell-escape",
          "%DOC%"
        ]
      }
    ]
  }
  